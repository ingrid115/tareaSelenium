<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression suite" verbose="0" time-out="60000">
    <groups>
        <run>
            <include name="Regression"/>
        </run>
    </groups>

    <listeners>
        <listener class-name="listeners.TestListeners"/>
        <listener class-name="listeners.SuiteListeners"/>
    </listeners>

    <test name="Tests de lectura de excel">
        <packages>
            <package name="excelReader"/>
        </packages>
    </test>

    <test name="Tests de faker">
        <packages>
            <package name="faker"/>
        </packages>
    </test>

    <test name="Tests de lectura json">
        <packages>
            <package name="jsonReader"/>
        </packages>
    </test>
</suite>